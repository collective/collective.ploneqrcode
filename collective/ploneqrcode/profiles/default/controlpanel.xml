<?xml version="1.0"?>
<object name="portal_controlpanel"
        meta_type="Plone Control Panel Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n"
        i18n:domain="collective.ploneqrcode"
        purge="False">
    <configlet title="ploneqrcode Settings"
        action_id="ploneqrcode"
        appId="collective.ploneqrcode"
        category="Products"
        condition_expr=""
        i18n:attributes="title"
        icon_expr="string:${portal_url}/++resource++collective.ploneqrcode/ploneqrcode.png"
        url_expr="string:${portal_url}/@@ploneqrcode-settings"
        visible="True">
     <permission>Manage portal</permission>
    </configlet>
</object>
